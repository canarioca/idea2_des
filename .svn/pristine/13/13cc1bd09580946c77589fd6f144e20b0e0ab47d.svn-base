<?xml version="1.0" encoding="ISO-8859-1" ?>
<ui:composition xmlns="http://www.w3.org/1999/xhtml"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:rich="http://richfaces.ajax4jsf.org/rich"
	xmlns:t="http://myfaces.apache.org/tomahawk"
	xmlns:a4j="http://richfaces.org/a4j"
	xmlns:f="http://java.sun.com/jsf/core">

	<h:panelGrid width="100%">
		
		<h:outputLabel value="Doctor" style="font-weight: bold;" />
		<h:selectOneMenu value="#{controlMarcapasos.obj.doctor}">
			<f:selectItems value="#{controlMarcapasos.ops }" />
		</h:selectOneMenu>
		<rich:spacer width="10" />
		<rich:spacer width="10" />
		<h:outputLabel value="Hospital" style="font-weight: bold;" />
		<h:selectOneMenu value="#{controlMarcapasos.obj.hospital}">
			<f:selectItems value="#{controlMarcapasos.centros }" />
		</h:selectOneMenu>
		<rich:spacer width="10" />
		<rich:spacer width="10" />
		<h:outputLabel value="Dirección" style="font-weight: bold;" />
		<h:inputText size="75" value="#{controlMarcapasos.obj.dirhosp }"/>
		<rich:spacer width="10" />
		<rich:spacer width="10" />
		<h:outputLabel value="Población" style="font-weight: bold;" />
		<h:inputText size="75" value="#{controlMarcapasos.obj.ciudadhosp }"/>
		<rich:spacer width="10" />
		<rich:spacer width="10" />
		<h:outputLabel value="Teléfono" style="font-weight: bold;" />
		<h:inputText size="75" value="#{controlMarcapasos.obj.tfnohosp }"/>
		<rich:spacer width="10" />
		<rich:spacer width="10" />
	</h:panelGrid>
</ui:composition>