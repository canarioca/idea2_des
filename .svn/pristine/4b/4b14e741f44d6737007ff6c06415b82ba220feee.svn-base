package com.sorin.idea.dto;

import java.util.Date;

public class RecallDto {

	private String modelo;
	private String numserie;
	private String fabricante;
	private Date fechaimp;
	private String nhc;
	private String modeloAct;
	private String numserieAct;
	private Integer estadoElec;
	private Integer caracterElec;
	
	public RecallDto(){
	}
	// constructor generador
	public RecallDto(String modelo, String numserie, String fabricante, Date fechaimp, 
			String nhc, String modeloAct, String numserieAct){
		this.modelo = modelo;
		this.numserie = numserie;
		this.fabricante = fabricante;
		this.fechaimp = fechaimp;
		this.nhc = nhc;
		this.modeloAct = modeloAct;
		this.numserieAct = numserieAct;
	}
	// constructor electrodo
	public RecallDto(String modelo, String numserie, String fabricante, Date fechaimp, String nhc, Integer estado, Integer caracter){
		this.modelo = modelo;
		this.numserie = numserie;
		this.fabricante = fabricante;
		this.fechaimp = fechaimp;
		this.nhc = nhc;
		this.estadoElec = estado;
		this.caracterElec = caracter;
	}
	
	// constructor registrador de eventos
	public RecallDto(String modelo, String numserie, String fabricante, Date fechaimp, String nhc){
		this.modelo = modelo;
		this.numserie = numserie;
		this.fabricante = fabricante;
		this.fechaimp = fechaimp;
		this.nhc = nhc;
	}
	
	public String getModelo() {
		return modelo;
	}
	public void setModelo(String modelo) {
		this.modelo = modelo;
	}
	public String getNumserie() {
		return numserie;
	}
	public void setNumserie(String numserie) {
		this.numserie = numserie;
	}
	public String getFabricante() {
		return fabricante;
	}
	public void setFabricante(String fabricante) {
		this.fabricante = fabricante;
	}
	public Date getFechaimp() {
		return fechaimp;
	}
	public void setFechaimp(Date fechaimp) {
		this.fechaimp = fechaimp;
	}
	public String getNhc() {
		return nhc;
	}
	public void setNhc(String nhc) {
		this.nhc = nhc;
	}

	public String getModeloAct() {
		return modeloAct;
	}

	public void setModeloAct(String modeloAct) {
		this.modeloAct = modeloAct;
	}

	public String getNumserieAct() {
		return numserieAct;
	}

	public void setNumserieAct(String numserieAct) {
		this.numserieAct = numserieAct;
	}

	public Integer getEstadoElec() {
		return estadoElec;
	}

	public void setEstadoElec(Integer estadoElec) {
		this.estadoElec = estadoElec;
	}

	public Integer getCaracterElec() {
		return caracterElec;
	}

	public void setCaracterElec(Integer caracterElec) {
		this.caracterElec = caracterElec;
	}
	
	
	
}
