	public ArrayList<ArrayList<Object>> getEstadisticasRegEvExt(Date fechaini, Date fechafin) throws RegistradorEventosExtDaoException{
		StringBuffer sql = new StringBuffer();
		sql.append("select regext.diagnosticofin DIAGNOSTICOFIN ");
		sql.append(" from procedimiento proc, ");
		sql.append(" registrador_eventos_ext regext ");
		sql.append(" WHERE proc.fecha between '"+UtilFechas.getFechaString(fechaini)+"' and '"+UtilFechas.getFechaString(fechafin)+"'");
		sql.append(" and proc.tipoprocedimiento = 1091 ");
		sql.append(" and proc.idprocedimiento = regext.idevento ");
		
		return findByDynamicSelect(sql.toString());
	}
	
