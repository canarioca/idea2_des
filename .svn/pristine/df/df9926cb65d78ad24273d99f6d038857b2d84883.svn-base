		if (episodio != null) {
			_hashCode = 29 * _hashCode + episodio.hashCode();
		}
		
		if (indicacionOtra != null) {
			_hashCode = 29 * _hashCode + indicacionOtra.hashCode();
		}
		
		if (epiSin != null) {
			_hashCode = 29 * _hashCode + epiSin.hashCode();
		}
		
		if (ntg != null) {
			_hashCode = 29 * _hashCode + ntg.hashCode();
		}
		
		return _hashCode;
	}

