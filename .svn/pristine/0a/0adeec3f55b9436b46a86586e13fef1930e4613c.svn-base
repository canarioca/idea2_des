package com.sorin.idea.util;

import java.io.Serializable;
import java.util.ArrayList;
import java.util.Date;

import com.sorin.idea.dto.Procedimiento;


public class ImplanteSeguimientoBean implements Serializable{

	private Procedimiento implante;
	private String tipoimplante;
	private String dispositivo;
	private String modelo;
	private Date fechaimplante;
	private ArrayList<ImplanteSeguimientoBean> seguimientos;
	private String comentarios_imp;
	private String comentarios_ev;
	private String comentarios_sitclin;
	private String comentarios_seg;
	private String comentarios_seg_internos;
	private String operador;
	
	public ImplanteSeguimientoBean(){
		this.setSeguimientos(new ArrayList<ImplanteSeguimientoBean>());
	}

	public Procedimiento getImplante() {
		return implante;
	}

	public void setImplante(Procedimiento implante) {
		this.implante = implante;
	}

	public ArrayList<ImplanteSeguimientoBean> getSeguimientos() {
		return seguimientos;
	}

	public void setSeguimientos(ArrayList<ImplanteSeguimientoBean> seguimientos) {
		this.seguimientos = seguimientos;
	}

	public String getTipoimplante() {
		return tipoimplante;
	}

	public void setTipoimplante(String tipoimplante) {
		this.tipoimplante = tipoimplante;
	}

	public Date getFechaimplante() {
		return fechaimplante;
	}

	public void setFechaimplante(Date fechaimplante) {
		this.fechaimplante = fechaimplante;
	}

	public String getDispositivo() {
		return dispositivo;
	}

	public void setDispositivo(String dispositivo) {
		this.dispositivo = dispositivo;
	}

	public String getModelo() {
		return modelo;
	}

	public void setModelo(String modelo) {
		this.modelo = modelo;
	}

	public String getComentarios_imp() {
		return comentarios_imp;
	}

	public void setComentarios_imp(String comentarios_imp) {
		this.comentarios_imp = comentarios_imp;
	}

	public String getComentarios_ev() {
		return comentarios_ev;
	}

	public void setComentarios_ev(String comentarios_ev) {
		this.comentarios_ev = comentarios_ev;
	}

	public String getComentarios_sitclin() {
		return comentarios_sitclin;
	}

	public void setComentarios_sitclin(String comentarios_sitclin) {
		this.comentarios_sitclin = comentarios_sitclin;
	}

	public String getOperador() {
		return operador;
	}

	public void setOperador(String operador) {
		this.operador = operador;
	}

	public String getComentarios_seg() {
		return comentarios_seg;
	}

	public void setComentarios_seg(String comentarios_seg) {
		this.comentarios_seg = comentarios_seg;
	}

	public String getComentarios_seg_internos() {
		return comentarios_seg_internos;
	}

	public void setComentarios_seg_internos(String comentarios_seg_internos) {
		this.comentarios_seg_internos = comentarios_seg_internos;
	}
}
