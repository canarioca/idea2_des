package com.sorin.idea.util;

import java.util.ArrayList;

/**
 * Clase bean para contener información de item de suministros para generación de 
 * informes de suministros
 * @author Francisco.Romera
 *
 */
public class Suministro {

	private String fabricante;
	private ArrayList<String> modelos;
	private ArrayList<String> codigos;
	private ArrayList<String> dispositivo;
	private int accion;
	
	public Suministro(){
		this.modelos = new ArrayList<String>();
		this.codigos = new ArrayList<String>();
		this.dispositivo = new ArrayList<String>();
	}

	public String getFabricante() {
		return fabricante;
	}

	public void setFabricante(String fabricante) {
		this.fabricante = fabricante;
	}

	public ArrayList<String> getModelos() {
		return modelos;
	}

	public void setModelos(ArrayList<String> modelos) {
		this.modelos = modelos;
	}

	public ArrayList<String> getCodigos() {
		return codigos;
	}

	public void setCodigos(ArrayList<String> codigos) {
		this.codigos = codigos;
	}

	public ArrayList<String> getDispositivo() {
		return dispositivo;
	}

	public void setDispositivo(ArrayList<String> dispositivo) {
		this.dispositivo = dispositivo;
	}

	public int getAccion() {
		return accion;
	}

	public void setAccion(int accion) {
		this.accion = accion;
	}
}
