	/** 
	 * Devuelve la consulta estadística de las Vias accesorias
	 */
	public ArrayList<ArrayList<Object>> getEstadisticas(String nhcs, String eefs) throws ArrEstudioViasDaoException{
		StringBuffer sql = new StringBuffer();
		
		sql.append("select pac.nhc PACIENTE,");
		sql.append("ee.nestudio NESTUDIO,");
		sql.append("m1.valor VIA,");
		sql.append("m2.valor SENTIDO,");
		sql.append("va.idloc LOC1,");
		sql.append("va.locs LOC2");
		sql.append(" from arr_pacientes pac, arr_estudio ee,");
		sql.append(" arr_estudio_vias va");
		sql.append(" LEFT OUTER JOIN arr_maestras m1 ON va.idvia = m1.codigo");
		sql.append(" LEFT OUTER JOIN arr_maestras m2 ON va.idsentido = m2.codigo");
		sql.append(" WHERE pac.codigo = ee.idpaciente");
		sql.append(" AND ee.idestudio = va.idestudio");
		if(nhcs.length()>0)
			sql.append(" and pac.nhc in " + nhcs);
		if(eefs.length()>0)
			sql.append(" and ee.idestudio in " + eefs);
		sql.append(" ORDER BY 1");
		
		return findByDynamicSelect(sql.toString());
	}
	
