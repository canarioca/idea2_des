.protSincopePerfilBajo {
	-moz-box-shadow: 0px 10px 14px -7px #276873;
	-webkit-box-shadow: 0px 10px 14px -7px #276873;
	box-shadow: 0px 10px 14px -7px #276873;
	background:-webkit-gradient(linear, left top, left bottom, color-stop(0.05, #84c294), color-stop(1, #84dca8));
	background:-moz-linear-gradient(top, #84c294 5%, #84dca8 100%);
	background:-webkit-linear-gradient(top, #84c294 5%, #84dca8 100%);
	background:-o-linear-gradient(top, #84c294 5%, #84dca8 100%);
	background:-ms-linear-gradient(top, #84c294 5%, #84dca8 100%);
	background:linear-gradient(to bottom, #84c294 5%, #84dca8 100%);
	filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#84c294', endColorstr='#84dca8',GradientType=0);
	background-color:#84c294;
	-moz-border-radius:8px;
	-webkit-border-radius:8px;
	border-radius:8px;
	display:inline-block;
	cursor:pointer;
	color:#ffffff;
	font-family:arial;
	font-size:20px;
	font-weight:bold;
	padding:12px 75px;
	text-decoration:none;
	text-shadow:0px 1px 0px #3d768a;
}
.protSincopePerfilBajo:hover {
	background:-webkit-gradient(linear, left top, left bottom, color-stop(0.05, #84dca8), color-stop(1, #84c294));
	background:-moz-linear-gradient(top, #84dca8 5%, #84c294 100%);
	background:-webkit-linear-gradient(top, #84dca8 5%, #84c294 100%);
	background:-o-linear-gradient(top, #84dca8 5%, #84c294 100%);
	background:-ms-linear-gradient(top, #84dca8 5%, #84c294 100%);
	background:linear-gradient(to bottom, #84dca8 5%, #84c294 100%);
	filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#84dca8', endColorstr='#84c294',GradientType=0);
	background-color:#6baeb8;
}
